<%include ./partials/header.ejs %>
<link href="https://fonts.googleapis.com/css?family=Istok+Web|Lobster" rel="stylesheet">
<link rel="stylesheet" href="/feed.css">
<div class="big ">

    <div class="edit ">
        <h1 ><i class="fa fa-venus-mars" aria-hidden="true"></i> Welcome to Swoosh! </h1>
        <p class="lead" id="textbelow">A Feline Dating App to find your loved one's!</p>
        <p class="lead">
            <a class="btn btn-outline-light btn-lg" href="/feed/newprofile" role="button">Let's Roll !</a>
        </p>
    </div>
</div>
<div class="container">
       <div class="row">

    <%feeds.forEach(function(feed){%>
           <div class ="col-lg-4">
        <div class="ui link cards">
            <div class="card">
                <div class="image">
                    <img src="<%= feed.image%>">
                </div>
                <div class="content">
                    <div class="header"><%=feed.username%></div>
                    <div class="meta">
                        <a>Friends</a>
                    </div>
                    <div class="description">
                        <%= feed.description.substring(0,50)%>...
                    </div>
                    <a class="btn btn-lg btn-success" href="/feed/<%=feed._id%>">About</a>
                </div>
                <div class="extra content">
      <span class="right floated">
       <%=feed.created.toDateString()%>
      </span>
                    <span>
        <i class="user icon"></i>
        75 Friends
      </span>
                </div>
            </div>
    </div>
     </div>
          <% }) %>

     </div>

</div>
<div class="parallax"></div>
<%include ./partials/footer.ejs %>
